//http://geeksquiz.com/insertion-sort/
Insertion sort

1,2,11,12,13,5,6
         |
         key
       j i
5,11,12,13
void insertionSort(int[] ar,int n) {
    int i=1;
    for(; i < n; i++) {
        int key= arr[i];
        int j= i-1;
        while(j >=0 && arr[j] > key){
            arr[j+1] = arr[j];
            j--;
        }
        arr[j+1] = key;
    }
}
