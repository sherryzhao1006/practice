public class Solution {
    /**
     * @param num: a rotated sorted array
     * @return: the minimum number in the array
     */
    public int findMin(int[] num) {
      int start = 0;
      int end = num.length-1;
      while(start < end) {
        int m = start + (end-start)/2;
        if(num[end] < num[m]) {
          start = m+1;
        } else if(num[end] > num[m]) {
          end = m;
        } else {
          end--;
        }
      }
      return num[start];
    } 
}
